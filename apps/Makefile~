LIB = ../lib

Malang : Malang.o ../include/variables.o $(LIB)/libconfirmQuestion $(LIB)/libsolveQuestion $(LIB)/libmakeQuestion
	gcc -o Malang Malang.o ../include/variables.o \
	-I../include \
	-L../lib -lconfirmQuestion -lsolveQuestion -lmakeQuestion `pkg-config --cflags --libs gtk+-2.0`
Malang.o : Malang.c
	gcc -c -o Malang.o Malang.c -I../include `pkg-config --cflags --libs gtk+-2.0`

clean :
	rm *.*~
